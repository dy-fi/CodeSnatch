<div class='row'>

    <div class='col-sm-4'>
        <img src='./resources/camera_icon.png' alt='image not available'>
    </div>

    <div class='col-sm-4 offset-sm-2 title-banner'>
        <div id='title-display'>
            <h1 id='title'><a id='Code'>CodeSnatch</a></h1>
            <span id='typed-title'></span>
        </div>
        <p>Write code anywhere... and run it too</p>
        <br>

        {{>file-form}}
    </div>
</div>

<script src='https://cdn.jsdelivr.net/npm/typed.js@2.0.9'></script>
<script>
  var typed = new Typed('#typed-title', {
    stringsElement: '#title',
    typeSpeed: 100,
    showCursor: false,
  });
</script>
