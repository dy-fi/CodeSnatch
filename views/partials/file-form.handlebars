<div id='fileInterface'>
    <form method="POST" action="/frame" encType='multipart/form-data'>
        <fieldset>

            <div class="form-group">
                <input type='file' name='file' id='file' class='btn btn-outline-success btn-block' accept="image/*">
            </div>

        </fieldset>
        <!-- BUTTON -->
        <div class="form-group">
            <button id='convert' class="btn btn-primary btn-block" type="submit" onclick='loading()'>Convert</button>
        </div>
    </form>
</div>
